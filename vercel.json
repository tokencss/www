{
  "redirects": [
    {
      "source": "/schema%40:version(\\d\\.\\d\\.\\d)",
      "destination": "/schema@:version"
    },
    {
      "source": "/schema",
      "destination": "https://schema.tokencss.com/"
    }
  ],
  "rewrites": [
    {
      "source": "/schema@:version(\\d\\.\\d\\.\\d)",
      "destination": "https://schema.tokencss.com/v/:version/schema.json"
    }
  ]
}
